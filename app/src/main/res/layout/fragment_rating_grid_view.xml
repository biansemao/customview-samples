<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:rating="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >

    <TextView
        android:id="@+id/tv_add_level"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="增加进度"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="100dp"
        />

    <com.sky.hyh.customviewsamples.customview.ratinggridbar.RatingGridView
        android:id="@+id/rgv_progress"
        android:layout_width="45dp"
        android:layout_height="30dp"
        android:layout_marginTop="200dp"
        android:layout_gravity="center_horizontal"
        rating:columeCount="3"
        rating:lineCount="2"
        rating:starbg="@drawable/fx_ic_star_grey"
        rating:starfront="@drawable/fx_ic_star_light"
        rating:starW="14dp"
        rating:starH="14dp"
        />


</FrameLayout>